{
  "name": "ev3-judge",
  "description": "Live judging for ev3-client",
  "repository": {
    "type": "git",
    "url": "git://github.com/ev3-js/ev3-judge.git"
  },
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "validate": "standard",
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "init:git": "git init && git add --all && git commit -am 'FIRST' && git remote add origin https://danleavitt0@github.com/ev3-js/ev3-judge.git && git push -u origin master ",
    "init:travis": "travis enable",
    "postinit": "npm install && npm run init:git && npm run init:travis",
    "test": "tape -r babel-register -r babel-polyfill test/*",
    "dev": "budo src/index.js -P -- -browserify-hmr",
    "start": "node server.js"
  },
  "dependencies": {
    "@f/create-action": "^1.1.1",
    "@f/domready": "^1.0.2",
    "@f/foreach-obj": "^1.1.1",
    "@f/map-obj": "^1.2.2",
    "@f/set-prop": "^1.0.0",
    "@f/splice": "^1.0.0",
    "babel-polyfill": "^6.8.0",
    "cors": "^2.7.1",
    "enroute": "^1.0.1",
    "express": "^4.13.4",
    "hashids": "^1.0.2",
    "jss-simple": "^0.3.1",
    "multi": "^0.1.3",
    "redux-effects-location": "^0.2.0",
    "socket.io": "^1.4.5",
    "string-template": "^1.0.0",
    "vdux": "^2.4.5",
    "vdux-form": "^1.0.4",
    "vdux-ui": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-polyfill": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.3.13",
    "babelify": "^7.3.0",
    "browserify-hmr": "^0.3.1",
    "redux-logger": "^2.6.1",
    "tape": "^4.2.2"
  },
  "bugs": {
    "url": "https://github.com/ev3-js/ev3-judge/issues"
  },
  "homepage": "https://github.com/ev3-js/ev3-judge#readme",
  "directories": {
    "test": "test"
  },
  "author": "danleavitt0"
}
