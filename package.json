{
  "name": "ev3-judge",
  "description": "Live judging for ev3-client",
  "repository": {
    "type": "git",
    "url": "git://github.com/ev3-js/ev3-judge.git"
  },
  "version": "0.1.1",
  "engines": {
    "node": "6.0.0"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build; npm run minify",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "validate": "standard",
    "clean": "rm -rf lib",
    "minify": "cat public/bundle.js | uglifyjs -o public/bundle.js",
    "build": "browserify src/client.js > public/bundle.js -t babelify",
    "init:git": "git init && git add --all && git commit -am 'FIRST' && git remote add origin https://danleavitt0@github.com/ev3-js/ev3-judge.git && git push -u origin master",
    "init:travis": "travis enable",
    "postinit": "npm install && npm run init:git && npm run init:travis",
    "test": "tape -r babel-register -r babel-polyfill test/*",
    "dev": "firebase serve & watchify src/client.js -o public/bundle.js -d -t babelify -p browserify-hmr",
    "pre-deploy": "npm run build && npm run minify",
    "deploy": "git push -u origin master && git push heroku master",
    "start": "node server.js"
  },
  "dependencies": {
    "@f/create-action": "^1.1.1",
    "@f/curry-once": "^1.3.2",
    "@f/domready": "^1.0.2",
    "@f/flatten-gen": "^2.0.0",
    "@f/foreach-obj": "^1.1.1",
    "@f/handle-actions": "^1.1.2",
    "@f/map": "^1.5.3",
    "@f/map-obj": "^1.2.2",
    "@f/omit": "^2.2.3",
    "@f/omit-prop": "^1.1.2",
    "@f/reduce-obj": "^1.0.0",
    "@f/set-prop": "^1.0.0",
    "@f/splice": "^1.0.0",
    "@f/switch": "^1.0.1",
    "@weo-edu/schema": "https://github.com/weo-edu/schema.git",
    "@weo-edu/validate": "https://github.com/weo-edu/validate.git",
    "babel-polyfill": "^6.8.0",
    "bigeval": "^1.2.1",
    "cors": "^2.7.1",
    "declarative-location": "^0.1.7",
    "enroute": "^1.0.1",
    "express": "^4.13.4",
    "firebase": "^3.0.3",
    "flatten": "^1.0.2",
    "hashids": "^1.0.2",
    "howler": "^1.1.29",
    "light-game": "^0.1.9",
    "multi": "^0.1.3",
    "redux-effects": "^0.4.3",
    "redux-effects-location": "^0.2.0",
    "redux-effects-timeout": "^0.1.7",
    "redux-flo": "^2.2.1",
    "socket.io": "^1.4.5",
    "string-template": "^1.0.0",
    "type": "0.0.3",
    "vdux": "^2.4.6",
    "vdux-containers": "^1.1.23",
    "vdux-fire": "^0.1.2",
    "vdux-form": "^1.0.6",
    "vdux-ui": "^1.2.35",
    "yoco": "^1.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.3.13",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "redux-logger": "^2.6.1",
    "standard": "*",
    "tape": "^4.2.2",
    "uglifyjs": "^2.4.10"
  },
  "bugs": {
    "url": "https://github.com/ev3-js/ev3-judge/issues"
  },
  "homepage": "https://github.com/ev3-js/ev3-judge#readme",
  "directories": {
    "test": "test"
  },
  "author": "danleavitt0"
}
